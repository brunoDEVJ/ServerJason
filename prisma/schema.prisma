generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model conexoestabela {
  CONEXOESTABELA_ID Int     @id @default(autoincrement())
  NOMETABELA        String  @unique(map: "NOMETABELA_UNIQUE") @db.VarChar(100)
  CODEMP            Int
  UP                String? @db.VarChar(1)
  DOWN              String? @db.VarChar(1)
  TERMINALINSERT    Int?    @default(0)
  TERMINALUPDATE    Int?    @default(0)
  TERMINALDELETE    Int?    @default(0)
  BANCOINSERT       Int?    @default(0)
  BANCOUPDATE       Int?    @default(0)
  BANCODELETE       Int?    @default(0)
}

model empresa {
  EMPRESA_ID   Int    @id @default(0)
  NOME_EMPRESA String @db.VarChar(255)
}

model grupoproduto {
  GRUPO_PRODUTO_ID      Int       @id @default(autoincrement())
  DESCRICAO             String    @unique @db.VarChar(50)
  MARGEMMINIMA          Float?
  MARGEMMAXIMA          Float?
  ATIVO                 String?   @db.Char(1)
  PERCENTUAL_MARKUP     Float?
  VENDEDOR_ALTERA_PRECO String?   @default("N") @db.VarChar(1)
  CODEMP                Int
  CNTINSERT             Int?
  CNTUPDATE             Int?
  DATAALTERACAO         DateTime? @db.Date
}

model numseq {
  NOMETABELA   String  @id @db.VarChar(30)
  NUMSEQTABELA Int
  CODEMP       Int
  FLGLOG       String? @db.Char(1)
}

model bairro {
  BAIRRO_ID     Int
  NOME_BAI      String    @db.VarChar(30)
  CNTINSERT     Int?      @default(0)
  CNTUPDATE     Int?      @default(0)
  DATAALTERACAO DateTime? @db.DateTime(0)
  CODEMP        Int?      @default(1)

  @@id([BAIRRO_ID, NOME_BAI])
}

model mapaatualizacoes {
  MAPA_DELETE_ID Int       @id @default(autoincrement())
  TABELA         String    @db.VarChar(45)
  CNTINSERT      Int?      @default(-1)
  CMD            String?   @db.VarChar(45)
  CODEMP         Int       @default(1)
  DATAALTERACAO  DateTime? @db.Date
}

